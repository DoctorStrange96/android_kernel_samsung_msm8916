		/* TS-in 0 B */
		/* ts-in-0-b-start */
		tsc_ts_in_0_b_start {
			qcom,pins = <&gp 73>;	/* ts_in_0_start_b */
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <1>;
			label = "tsc-ts-in-0-b-start";
			ts_in_0_b_start_active: ts_in_0_b_start_active {
				drive-strength = <2>;	/* 2mA */

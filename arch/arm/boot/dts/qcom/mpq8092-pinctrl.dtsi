			qcom,pins = <&gp 100>;	/* ts_in_0_valid_a */
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <2>;
			label = "tsc-ts-in-0-a-valid";
			ts_in_0_a_valid_active: ts_in_0_a_valid_active {
				drive-strength = <2>;	/* 2mA */
				bias-pull-down;

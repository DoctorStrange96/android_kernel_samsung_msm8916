			qcom,pins = <&gp 101>;	/* ts_in_0_err_a */
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <2>;
			label = "tsc-ts-in-0-a-err";
			ts_in_0_a_err_active: ts_in_0_a_err_active {

			ts_in_0_b_par_2_sleep: ts_in_0_b_par_2_sleep {
				drive-strength = <2>;	/* 2mA */
				bias-disable;
			};
		};

		tsc_ts_in_0_b_par_3 {
			qcom,pins = <&gp 68>,	/* ts_in_0_data_b[6] */
						<&gp 69>;	/* ts_in_0_data_b[7] */
			qcom,num-grp-pins = <2>;
			qcom,pin-func = <3>;
			label = "ts-in-0-b-par3";
			ts_in_0_b_par_3_active: ts_in_0_b_par_3_active {
				drive-strength = <2>;	/* 2mA */
